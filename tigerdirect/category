/* <![CDATA[ */
function GetSearchPage(val)   {
   var srchPage   = '../searchtools/search.asp?cat=6957&noredirect=1&mnf=0';
   var lo = GetLow(val);
   var hi = GetHigh(val);
   srchPage += '&lowprice='+ lo  + '&highprice='+ hi;
   location.href=srchPage;
}

function GetLow(val) { // Get the High value from the option
   var lo = val.substr(0, val.indexOf("|"));
   return lo;
}
function GetHigh(val) { // Get the Low value from the option
   var hi = val.substr(val.indexOf("|")+1);
   return hi;
}

function getCookie_Compare(name) {
   var   start =  document.cookie.indexOf(name+"=");
   var   len   = start+name.length+1;
   if ((!start) &&   (name != document.cookie.substring(0,name.length)))   return null;
   if (start == -1) return null;
   var   end   = document.cookie.indexOf(";",len);
   if (end  == -1) end = document.cookie.length;
   return unescape(document.cookie.substring(len,end));
}

function setCookie_Compare(on,name,value,expires,path,domain,secure) {
   uValue = getCookie_Compare( name   );
   if(on) {
     if( uValue ) {
		var CookieVal   = uValue + ",";		
		 if(CookieVal.indexOf(value + ",") >= 0 ){
		 	value = uValue
		 }
		 else {
			value = uValue +  ","   + value
		 }
	 }
   }
   else  {
     var CookieArr   = uValue.split(",")
     for (var i = 0; i  < CookieArr.length;  i++) {
       if( value == CookieArr[i] ) {
         delete CookieArr[i];
         break;
       }
     }
     value  = CookieArr.join(",");
   }
   document.cookie = name +   "="   + escape(value)   +
    ( (expires) ? ";expires=" + expires.toGMTString() : "") +
    ( (path) ? ";path=" + path   : "") +  
    ( (domain) ? ";domain=" + domain : "")   +
    ( (secure) ? ";secure" : "");
}

function deleteCookie_Compare(name) {
   var exp = new Date();
   exp.setTime (exp.getTime() -  1000000000);  // This cookie is  history  (changed -1 to make  it previous time)
   var cval = getCookie_Compare(name);
   alert(cval);
   document.cookie = name +   "="   + cval + "; expires=" + exp.toGMTString();
}
function popGenuine() 
{
   var pct=0.85, wth=500, hgt=500;  
   if (parseInt(navigator.appVersion) > 3) {
      wth = parseInt(screen.availWidth * pct);
      hgt  = parseInt(screen.availHeight * pct);
   }
   window.open('/sectors/microsoft/genuine.asp','name','height=' + hgt + ',width=' + wth);
}
function popMSGenuine() 
{
   var pct=0.85, wth=500, hgt=500;  
   if (parseInt(navigator.appVersion) > 3) {
      wth = parseInt(screen.availWidth * pct);
      hgt  = parseInt(screen.availHeight * pct);
   }
   window.open('http://www.microsoft.com/genuine','name','height=' + hgt + ',width=' + wth);
}

