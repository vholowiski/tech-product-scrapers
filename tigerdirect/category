<script charset="utf-8" type="text/javascript">
function getUserIdRR() {
  try{
		var cookie = $.cookie('DB');
       if(cookie==undefined) {return $.cookie('tdssid');};
				var pairs = cookie.split('&');
				for (i = 0; i < pairs.length; i++) 
				{
					var pair = pairs[i].split('=');
					if (pair.length != 2) continue;
					if (pair[0] === 'euqa' && pair[1])
					{
						return pair[1];
					}
				}
				return $.cookie('tdssid');
  }catch (e){return $.cookie('tdssid');}
};
function getSessionIdRR() {
  try{
    return $.cookie('tdssid');
  }catch (e){return '';}
};
function getTDSSID() {
  try{
       var d = new Date().getTime();
       var uid = 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function(c) {
           var r = (d + Math.random()*16)%16 | 0;
           d = Math.floor(d/16);
           return (c=='x' ? r : (r&0x3|0x8)).toString(16);
       });
        return uid;
     }catch (e){return '';}
};
(function() {
 try{
  if($.cookie('tdssid')==undefined)
   {
    var tdssid_d = new Date();
    tdssid_d.setTime(tdssid_d.getTime() + (3*60*60*1000));
    var tdssid_u = getTDSSID(); 
    $.cookie('tdssid', tdssid_u,{expires: tdssid_d}); 
    
   }
  }catch (e){};
 })(); 
</script>
