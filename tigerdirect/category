<script>
	$(function(){			
		$("[data-id=mastNav_flyout_1] li.mastNav-cat[data-attributes]").each(function( index ) {
			var attributes = $(this).data("attributes");
			
			if(attributes){
				if(attributes.IsNew){
					$(this).prepend('<span class="new-cat">New</span>');
				}
				if(attributes.Height){
					var target = $(this).data("target");
					if(target){
						$("#" + target).height(attributes.Height);
					}
				}
			}
		});
	});
</script>
<script>
//MAST NAVIGATION action
//if mastnav elem exists
if(document.getElementById('mast_nav')){
//mastnav delay
	$(function() {
		var timer;
		$('li.navItem').on("mouseover mouseenter", function(event) {
			var $menuPop = $(this);
			if(timer) {
				clearTimeout(timer);
				timer = null
			}
			timer = setTimeout(function() {
				$menuPop.addClass('jHover');
			}, 150)
		}); 
		// mouse out
		$('li.navItem').on("mouseleave", function(event) {
			$(this).removeClass('jHover');
			if(timer) {
				clearTimeout(timer);
				timer = null
			}
		});
		//show mastnav also on click
		$("li.navItem").on("click",function(event) {
			$(this).addClass('jHover');
		});
	});
}

//mastnav flyout menu
$(document).ready(function() {	
	var $menu = $(".mastNav-menu"),
	$showOnce = 1;
	$menu.menuAim({
		// fired on row activation
		activate: function(row){
			var   idx = $(row).index(),
			submenuId = $(row).parent().data("id"),
			$submenu  = $("#" + submenuId);
			$(row).addClass('hover');
			$submenu.find('.mastNav-subWrap').css('min-height', $menu.height());
			$submenu.find('.mastNav-subWrap').stop(true, true).hide();
			if($showOnce){
				$submenu.find('.mastNav-subWrap').eq(idx).stop(true, true).show('slide', {direction: 'left'}, 100);
				$showOnce = 0;
			}else{
				$submenu.find('.mastNav-subWrap').eq(idx).show();
			}
		},  
		// fired on row deactivation
		deactivate: function(row){
			var idx = $(row).index(),
				submenuId = $(row).parent().data("id"),
				$submenu = $("#" + submenuId);
			$(row).removeClass('hover');
			$submenu.find('.mastNav-subWrap').eq(idx).hide();
		},
		// fired on menu exit
		exitMenu: function(){
			$showOnce = 1;
		}
	});
});



</script>
<script src="/sectors/campaigns/jquery/jquery.maskedinput.js" type="text/javascript"></script>
